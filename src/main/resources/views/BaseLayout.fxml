                        <ImageView fitHeight="195.0" fitWidth="238.0" layoutX="14.0" layoutY="-15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/unap_negativo.png" />
                           </image>
                        </ImageView>
                     </children>
                  </Pane>
                  <Button id="BotonInicio" fx:id="BotonInicio" layoutX="2.0" layoutY="290.0" mnemonicParsing="false" onAction="#GoInicio" prefHeight="42.0" prefWidth="266.0" styleClass="BotonPanelLateral" stylesheets="@../css/BaseLayout.css" text="Inicio" textFill="WHITE">
                     <font>
                        <Font name="Product Sans" size="18.0" />
                     </font>
                  </Button>
                  <Button id="BotonLab" fx:id="BotonLaboratorios" layoutX="2.0" layoutY="391.0" mnemonicParsing="false" onAction="#GoLaboratorios" prefHeight="42.0" prefWidth="266.0" styleClass="BotonPanelLateral" stylesheets="@../css/BaseLayout.css" text="Laboratorios" textFill="WHITE">
                     <font>
                        <Font name="Product Sans" size="18.0" />
                     </font>
                  </Button>
                  <Button id="BotonEquipo" fx:id="BotonEquipos" layoutX="2.0" layoutY="440.0" mnemonicParsing="false" onAction="#GoEquipos" prefHeight="42.0" prefWidth="266.0" styleClass="BotonPanelLateral" stylesheets="@../css/BaseLayout.css" text="Equipos" textFill="WHITE">
                     <font>
                        <Font name="Product Sans" size="18.0" />
                     </font>
                  </Button>
                  <Button id="BotonUsuarios" fx:id="BotonUsuarios" layoutX="1.0" layoutY="490.0" mnemonicParsing="false" onAction="#GoUsuarios" prefHeight="42.0" prefWidth="266.0" styleClass="BotonPanelLateral" stylesheets="@../css/BaseLayout.css" text="Usuarios" textFill="WHITE">
                     <font>
                        <Font name="Product Sans" size="18.0" />
